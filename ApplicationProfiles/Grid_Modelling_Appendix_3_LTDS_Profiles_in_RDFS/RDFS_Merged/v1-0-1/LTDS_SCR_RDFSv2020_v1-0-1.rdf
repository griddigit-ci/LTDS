  <rdf:Description rdf:about="#UnitSymbol.deg">
    <rdfs:comment>Plane angle in degrees.</rdfs:comment>
    <rdfs:label xml:lang="en">deg</rdfs:label>
    <cims:stereotype>enum</cims:stereotype>
    <rdf:type rdf:resource="#UnitSymbol"/>
